    var userId, params;

    window.ga = window.ga || function () {
            (ga.q = ga.q || []).push(arguments)
        };
    ga.l = +new Date;

    

    if (userId) {
        ga('create', 'UA-36321328-3', 'builds.io', params);
        ga('set', 'dimension2', userId);
    } else {
        ga('create', 'UA-36321328-3', 'builds.io');
    }

    ga('require', 'displayfeatures');

    ga(function (tracker) {
        var clientID = tracker.get('clientId'); //Получаем ID пользователя
        document.cookie = "_ga_cid=" + clientID + "; path=/"; // Сохраняем в куку
        ga('set', 'dimension1', clientID); //Сохраняем ID в переменную в GA

        if (userId) {
            params['dimension1'] = clientID;
        } else {
            params = {
                'dimension1': clientID
            }
        }

        if (userId) {
            ga('send', 'pageview', params);
        } else {
            ga('send', 'pageview');
        }
    });
</script>
